{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@appc/*": ["./src/app/_components/*"],
      "@mainc/*": ["./src/app/(main)/_components/*"],
      "@/*": ["./src/*"],
      "tailwind.config": ["./tailwind.config.js"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "nativewind-env.d.ts"]
}
