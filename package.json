{
  "name": "hinomaru-wallet-app",
  "version": "0.1.0",
  "description": "HinomaruWallet is a non-custodial mobile wallet focused on stablecoin with ERC-4337 support, Paymaster gas abstraction, and simple DeFi features on Base.",
  "author": "FickleWolf",
  "main": "./src/index.ts",
  "scripts": {
    "start": "expo start",
    "android:dev": "dotenv -e .env.dev -- expo run:android",
    "android:prd": "dotenv -e .env.prd -- expo run:android",
    "ios:dev": "dotenv -e .env.dev -- expo run:ios",
    "ios:prd": "dotenv -e .env.prd -- expo run:ios",
    "test": "jest --watchAll",
    "lint": "expo lint --fix",
    "format": "npx prettier --write '**/*.{js,ts,tsx}'",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
