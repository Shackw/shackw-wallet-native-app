{
  "name": "shackw-wallet-native-app",
  "version": "0.2.1",
  "description": "ShackwWallet is a non-custodial mobile wallet focused on stablecoin with EIP-7702 support.",
  "author": "Shackw",
  "main": "./src/index.ts",
  "scripts": {
    "lint": "expo lint --fix && tsc --noEmit",
    "format": "npx prettier --write '**/*.{js,ts,tsx}'",
    "check": "yarn format && yarn lint",
    "dev:android": "dotenv -e .env -- expo run:android",
    "build:android:apk:local": "yarn check && npx eas-cli build --platform android --profile android-internal --local",
    "build:android:aab": "yarn check && npx eas-cli build --platform android --profile production",
    "build:ios:ipa": "yarn check && npx eas-cli build --platform ios --profile production  --auto-submit",
    "creds:android": "npx eas-cli credentials --platform android",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/html-elements": "^0.12.5",
