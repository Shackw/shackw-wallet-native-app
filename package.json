    "@tanstack/react-form": "^1.27.2",
    "@tanstack/react-query": "^5.90.12",
    "@walletconnect/react-native-compat": "^2.23.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "buffer": "^6.0.3",
    "date-fns": "^4.1.0",
