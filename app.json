{
  "expo": {
    "name": "Hinomaru Wallet",
    "slug": "hinomaru-wallet",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "com.ficklewolf.hinomaruwallet",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.ficklewolf.hinomaruwallet",
      "associatedDomains": ["applinks:ficklewolf.com"],
      "entitlements": {
        "com.apple.developer.associated-domains": ["applinks:ficklewolf.com"]
      },
      "supportsTablet": true
    },
    "android": {
      "package": "com.ficklewolf.hinomaruwallet",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            { "scheme": "https", "host": "ficklewolf.com", "pathPrefix": "/wallet/addresses" },
            { "scheme": "https", "host": "ficklewolf.com", "pathPrefix": "/wallet/transfer" }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            "extraPods": [
              {
                "name": "OpenSSL-Universal",
                "configurations": ["Release", "Debug"],
                "modular_headers": true,
                "version": "3.3.2000"
              }
            ]
          }
        }
      ],
      [
        "expo-sqlite",
        {
          "enableFTS": true,
          "useSQLCipher": true,
          "android": {
            "enableFTS": false,
            "useSQLCipher": false
          },
          "ios": {
            "customBuildFlags": ["-DSQLITE_ENABLE_DBSTAT_VTAB=1 -DSQLITE_ENABLE_SNAPSHOT=1"]
          }
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d1d7acaf-b44a-4425-af35-620596448499"
      }
    },
    "owner": "FickleWolf"
  }
}
